{
  "title": "deno-csv2embedding",
  "description": "This project helps to create embeddings from CSV data by OpenAI's API.",
  "tasks": {
    "build": "deno run --allow-write --allow-net --allow-env --allow-read src/main.ts",
    "clean": "deno run --allow-write _tasks/clean.ts"
  },
  "lint": {
    "files": {
      "include": [
        "src"
      ]
    }
  },
  "fmt": {
    "files": {
      "include": [
        "src"
      ]
    }
  },
  "test": {
    "files": {
      "include": ["src/"],
      "exclude": ["src/testdata/"]
    }
  }
}
